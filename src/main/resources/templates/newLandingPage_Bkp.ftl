<body class="front-page border border-white" style="background-color: lightslategray;border-width: 20px!important;">
  <div class="pt-5" style="min-height:100vh;">
    <section class="pv-30 clearfix" style="background-color: lightslategray;">
      <div class="container">
        <div class="row">
          <div class="col-lg-8">
            <div class="row justify-content-md-center">
              <div class="col-lg-8">
                <br>
                <br>
                <br>
                <br>
                <br>
                <h2 class="text-center mt-4 text-light"><strong>Project & Inventory Management</strong></h2>
                <div class="separator"></div>
                <p class="large text-center text-light">Efficient way of managing Project workflow and Inventory</p>
              </div>
            </div>
          </div>

          <div class="col-lg-4">
            <div class="pv-30 ph-20 feature-box bordered shadow text-center object-non-visible" data-animation-effect="fadeInDownSmall" data-effect-delay="200">
              <h3>Login</h3>
              <form id="updateInv" action="submit" method="POST">                
                <label for="inputUserName" class="text-md-right control-label col-form-label font-weight-bold">User Name</label>
                <input class="form-control" type="userName" name="userName" id="inputUserName" style="width:100%;">
                <label for="inputPassword" class="text-md-right control-label col-form-label font-weight-bold">Password</label>
                <input class="form-control" type="Password" name="password" id="inputPassword" style="width:100%;">
                <button type="submit" class="btn btn-default" style="background-color: #778899;">Log In </button>
              </form>
              <div class="separator clearfix"></div>
              <p><a href="resetPassword">Forgot Password?</a></p>
            </div>
          </div>
        </div>
      </div>
      <div class="container pv-45">
          <div class="subfooter-inner">
            <div class="row">
              <div class="col-md-12">
                <p class="text-center text-light">Powered By : Social Angels Digital Co.</p>
              </div>
            </div>
          </div>
        </div>
    </section>
